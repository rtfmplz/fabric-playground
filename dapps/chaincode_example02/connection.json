{
    "name": "fabric-playground",
    "version": "1.0.0",
    "client": {
        "organization": "Org1",
        "connection": {
            "timeout": {
                "peer": {
                    "endorser": "300"
                },
                "orderer": "300"
            }
        }
    },
    "channels": {
        "ch1": {
            "orderers": [
                "orderer1"
            ],
            "peers": {
                "peer1.org1": {}
            }
        }
    },
    "organizations": {
        "Org1": {
            "mspid": "Org1MSP",
            "peers": [
                "peer1.org1",
                "peer2.org1"
            ],
			"fullpath": false,
            "certificateAuthorities": [
                "fabric-ca"
            ]
        }
    },
    "orderers": {
        "orderer1": {
            "url": "grpcs://orderer1.ordererorg:7050",
			"tlsCACerts": {
				"path": "./crypto/ordererOrganizations/ordererorg/tlsca/tlsca.ordererorg-cert.pem"
			}
        }
    },
    "peers": {
        "peer1.org1": {
            "url": "grpcs://peer1.org1:7051",
			"tlsCACerts": {
				"path": "./crypto/peerOrganizations/org1/tlsca/tlsca.org1-cert.pem"
			}
        }
    },
    "certificateAuthorities": {
        "fabric-ca": {
            "url": "http://localhost:7054",
            "caName": "fabric-ca"
        }
    }
}

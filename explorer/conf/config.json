{
    "network-configs": {
      "network-1": {
        "version": "1.0",
        "clients": {
          "client-1": {
            "tlsEnable": false,
            "organization": "Org1",
            "channel": "ch1",
            "credentialStore": {
              "path": "./tmp/credentialStore_Org1/credential",
              "cryptoStore": {
                "path": "./tmp/credentialStore_Org1/crypto"
              }
            }
          }
        },
        "channels": {
          "ch1": {
            "peers": {
              "peer1.org1": {},
              "peer2.org1": {}
            },
            "connection": {
              "timeout": {
                "peer": {
                  "endorser": "6000",
                  "eventHub": "6000",
                  "eventReg": "6000"
                }
              }
            }
          }
        },
        "organizations": {
          "Org1": {
            "mspid": "Org1MSP",
            "fullpath": false,
            "adminPrivateKey": {
              "path": "/tmp/crypto/peerOrganizations/org1/users/Admin@org1/msp/keystore"
            },
            "signedCert": {
              "path": "/tmp/crypto/peerOrganizations/org1/users/Admin@org1/msp/signcerts"
            },
            "certificateAuthorities": ["fabric-ca"],
            "peers": ["peer1.org1", "peer2.org1"]
          },
          "OrdererMSP": {
            "mspid": "OrdererOrgMSP",
            "adminPrivateKey": {
              "path": "/tmp/crypto/ordererOrganizations/ordererorg/users/Admin@ordererorg/msp/keystore"
            }
          }
        },
        "peers": {
          "peer1.org1": {
            "tlsCACerts": {
              "path": "/tmp/crypto/peerOrganizations/org1/peers/peer1.org1/tls/ca.crt"
            },
            "url": "grpcs://peer1.org1:7051",
            "eventUrl": "grpcs://peer1.org1:7053",
            "grpcOptions": {
              "ssl-target-name-override": "peer1.org1"
            }
          },
          "peer2.org1": {
            "tlsCACerts": {
              "path": "/tmp/crypto/peerOrganizations/org1/peers/peer2.org1/tls/ca.crt"
            },
            "url": "grpcs://peer2.org1:7051",
            "eventUrl": "grpcs://peer2.org1:7053",
            "grpcOptions": {
              "ssl-target-name-override": "peer2.org1"
            }
          }
        },
        "orderers": {
          "orderer1": {
            "url": "grpcs://orderer1.ordererorg:7050"
          }
        },
        "certificateAuthorities": {
          "fabric-ca": {
            "url": "https://fabric-ca:7054",
            "httpOptions": {
              "verify": false
            },
            "tlsCACerts": {
              "path": "/tmp/crypto/peerOrganizations/org1/ca/ca.org1-cert.pem"
            },
            "caName": "fabric-ca"
          }
        }
      },
      "network-2": {}
    },
    "configtxgenToolPath": "/tmp/fab-bin/",
    "license": "Apache-2.0"
  }
  